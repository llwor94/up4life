{
	"version": 2,
	"name": "up4",
	"builds": [
		{ "src": "next.config.js", "use": "@now/next@canary" },
		{
			"src": "static/*",
			"use": "@now/static"
		}
	],
	"routes": [
		{ "src": "/static/(.*)", "dest": "static/$1" },
		{ "src": "/(.*)", "dest": "pages/$1" },
		{ "src": "/profile/(?<slug>.+)$", "dest": "/profile?slug=$slug" },
		{ "src": "/welcome/(?<slug>.+)$", "dest": "/welcome?slug=$slug" }
	],
	"alias": [ "up4.life", "www.up4.life" ]
}
